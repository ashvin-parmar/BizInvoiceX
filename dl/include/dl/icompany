#ifndef __$__icompany__dl 
#define __$__icompany__dl 123
#include<iostream>
using namespace std;
namespace invoicegenerator
{
namespace data_layer
{
namespace abc
{
class ICompany
{
public:
virtual void setName(string Name)=0;
virtual void setStreet(string street)=0;
virtual void setCity(string city)=0;
virtual void setState(string state)=0;
virtual void setPinCode(int pinCode)=0;
virtual void setContactNumber(string contactNumber)=0;
virtual void setEmail(string email)=0;
virtual void setGSTNumber(string gstNumber)=0;
virtual void setPassword(string password)=0;
virtual void setUPIID(string UPIID)=0;
virtual void setCountry(string country)=0;
virtual string getCountry() const=0;
virtual string getName() const=0;
virtual string getStreet() const=0;
virtual string getCity() const=0;
virtual string getState() const=0;
virtual string getEmail() const=0;
virtual string getContactNumber() const=0;
virtual string getGSTNumber() const=0;
virtual string getUPIID() const=0;
virtual string getPassword() const=0;
virtual int getPinCode() const=0;
virtual void setCode(int code)=0;
virtual int getCode() const=0;
virtual ICompany & operator=(const ICompany &other)=0;
virtual int operator==(const ICompany &other)=0;
virtual int operator<=(const ICompany &other)=0;
virtual int operator>=(const ICompany &other)=0;
virtual int operator!=(const ICompany &other)=0;
virtual int operator<(const ICompany &other)=0;
virtual int operator>(const ICompany &other)=0;
};
}
}
}
#endif